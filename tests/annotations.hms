import trigger minute from triggers;

let foo = 1;

fn fib(n: int) -> int {
    if n < 2 {
        n
    } else {
        fib(n - 2) + fib(n - 1)
    }
}

#[trigger in minute(foo * 20 + fib(10))]
// #[trigger in minute($Driver.foo)]
event fn whatever(_elapsed: int) {
    println("whatever() was called");
}

fn main() {
    println("Hello world");
}
