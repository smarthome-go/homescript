// ImportStatement
import import_me from export;

fn importStmt() {
    assert(42 == import_me());
}

fn typeDef() {
    type User = {
        name: str,
        password: str,
    };

    let me: User = '{"name": "foo", "password": "bar"}'.parse_json();
    assert(me == new { name: "foo", password: "bar" });
}

fn main() {
    let tests = [
        importStmt,
    ];


    for test in tests {
        test();
    }

    typeDef();
    importStmt();
}
