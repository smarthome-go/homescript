# Different kinds of expressions
true || false; false || true;      # Or expr
true && false; true && true;       # And expr
1 == 1; 1 !== 2;                   # Eq expr
1 < 2; 1 <= 2;                     # Rel expr (less)
3 > 2; 3 >= 2;                     # Rel expr (greater)
1 + 1; 2 - 1;                      # Add expr
2 * 1; 2 / 1; 4 % 2;               # Mul expr
1 as str; 0 as bool; false as str; # Cast expr
-----1; +-+2;                      # Unary expr
2 ** 3;                            # Exp expr

1 * (-1 + 2) % 5;                  # Combined
