fn box(dimensions) {
    let output = "";
    for y in 0..dimensions {
        for x in 0..dimensions*2 {
            if x == 0 || y == 0 || x == dimensions*2-1 || y == dimensions-1 {
                output += "#";
            } else {
                output += " ";
            };
        };
        output += "\n";
    };
    return output;
};

for size in 2..8 {
    println(box(size));
};
